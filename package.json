{
  "name": "cookie",
  "version": "3.4.0",
  "description": "Graphics template for The Michigan Daily",
  "author": {
    "name": "Naitian Zhou"
  },
  "contributors": [
    {
      "name": "Eric Lau"
    }
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "sink fetch",
    "clean": "rm -rf dist/* && rm -rf .parcel-cache",
    "dev": "yarn clean && parcel src/index.html src/graphic/*.html",
    "prebuild": "mkdir -p img",
    "build": "yarn clean && node build.mjs",
    "postbuild": "if [ \"$(ls -A img/)\" ]; then ((cd dist; mkdir -p img) && mv img/* dist/img/); fi"
  },
  "targets": {
    "default": {
      "publicUrl": "/"
    }
  },
  "dependencies": {
    "d3": "^7.6.1",
    "dom-to-svg": "^0.12.2",
    "html-to-image": "^1.10.8",
    "pym.js": "^1.3.2"
  },
  "devDependencies": {
    "@michigandaily/parcel-transformer-dsv": "michigandaily/parcel-transformer-dsv#v2.0.1",
    "@michigandaily/parcel-transformer-nunjucks": "michigandaily/parcel-transformer-nunjucks#v2.2.4",
    "@parcel/core": "~2.8.0",
    "@parcel/transformer-sass": "~2.8.0",
    "buffer": "^5.5.0",
    "dotenv": "^16.0.3",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^7.0.4",
    "parcel": "~2.8.0",
    "playwright-core": "^1.27.1",
    "prettier": "2.7.1",
    "process": "^0.11.10",
    "sink": "michigandaily/sink#2.4.0"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
