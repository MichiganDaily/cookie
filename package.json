{
  "name": "cookie",
  "version": "2.2.0",
  "description": "Graphics template for The Michigan Daily",
  "type": "module",
  "author": {
    "name": "Naitian Zhou"
  },
  "contributors": [
    {
      "name": "Eric Lau"
    }
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf dist/* && rm -rf .parcel-cache",
    "start": "yarn clean && parcel src/index.html src/graphic/index.html",
    "build": "yarn clean && parcel build --no-scope-hoist src/index.html src/graphic/index.html"
  },
  "dependencies": {
    "d3": "^7.4.4",
    "pym.js": "^1.3.2"
  },
  "devDependencies": {
    "@michigandaily/parcel-transformer-nunjucks": "https://github.com/MichiganDaily/parcel-transformer-nunjucks",
    "@parcel/transformer-raw": "^2.4.1",
    "@parcel/transformer-sass": "^2.4.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "parcel": "^2.4.1",
    "sink": "https://github.com/MichiganDaily/sink"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
