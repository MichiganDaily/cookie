{
  "name": "cookie",
  "version": "2.4.0",
  "description": "Graphics template for The Michigan Daily",
  "author": {
    "name": "Naitian Zhou"
  },
  "contributors": [
    {
      "name": "Eric Lau"
    }
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf dist/* && rm -rf .parcel-cache",
    "dev": "yarn clean && parcel src/index.html src/graphic/index.html",
    "screengrab": "rm -rf img/* && node screengrab.mjs",
    "build": "yarn clean && parcel build --no-scope-hoist src/index.html src/graphic/index.html"
  },
  "dependencies": {
    "d3": "^7.4.4",
    "html-to-image": "^1.9.0",
    "pym.js": "^1.3.2"
  },
  "devDependencies": {
    "@michigandaily/parcel-transformer-nunjucks": "https://github.com/MichiganDaily/parcel-transformer-nunjucks.git",
    "@parcel/transformer-raw": "~2.6.0",
    "@parcel/transformer-sass": "~2.6.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "find-chrome-bin": "^1.0.2",
    "husky": "^7.0.4",
    "parcel": "~2.6.0",
    "prettier": "2.7.1",
    "process": "^0.11.10",
    "puppeteer-core": "^16.1.0",
    "sink": "https://github.com/MichiganDaily/sink"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
